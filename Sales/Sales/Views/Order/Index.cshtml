
@{
    ViewBag.Title = "Index";
}
<body>

<div>
    <table>
        <tr>
            <td>客戶代號</td>
            <td>客戶名稱</td>
            <td>負責員工</td>
        </tr>
        @foreach (var item in (List<Sales.Models.Order>)ViewBag.Data)
        {
            <tr>
                <td>@item.CustId</td>
                <td>@item.CustName</td>
                <td>@item.EmpName</td>
            </tr>
        }

    </table>

</div>

</body>

